{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@nomadreservations/ngx-stripe",
  "version": "1.2.3",
  "description": "@angular wrapper for StripeJS",
  "repository": {
    "type": "git",
    "url": "https://github.com/nomadreservations/ngx-stripe.git"
  },
  "bugs": {
    "url": "https://github.com/nomadreservations/ngx-stripe/issues"
  },
  "homepage": "https://github.com/nomadreservations/ngx-stripe",
  "keywords": [
    "angular",
    "javascript",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:release": "ng-packagr -p package.json",
    "release": "release-it",
    "release:major": "npm run release -- major -n",
    "release:minor": "npm run release -- minor -n",
    "release:patch": "npm run release -- patch -n",
    "compodoc": "compodoc -p tsconfig.json",
    "compodoc:serve": "compodoc -p tsconfig.json -sw",
    "compodoc:watch": "compodoc -p tsconfig.json -w",
    "docs": "npm-run-all -s compodoc:serve",
    "docs:watch": "npm-run-all -p compodoc:serve compodoc:watch",
    "docs:publish": "run-s demo:aot:yalc && bash deploy-docs.sh",
    "doctoc": "doctoc README.md --maxlevel 3"
  },
  "peerDependencies": {},
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.15",
    "@angular/animations": "~13.1.2",
    "@angular/cdk": "^12.2.13",
    "@angular/cli": "~12.2.15",
    "@angular/common": "~13.1.2",
    "@angular/compiler": "~13.1.2",
    "@angular/compiler-cli": "~13.1.2",
    "@angular/core": "~12.2.15",
    "@angular/forms": "~13.1.2",
    "@angular/language-service": "~13.1.2",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~13.1.2",
    "@angular/platform-browser-dynamic": "~13.1.2",
    "@angular/router": "~13.1.2",
    "@compodoc/compodoc": "^1.1.10",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "auto-changelog": "^1.16.1",
    "codelyzer": "^6.0.0",
    "core-js": "^3.2.1",
    "doctoc": "^1.4.0",
    "fstream": "^1.0.12",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "js-yaml": "^3.13.1",
    "karma": "~6.3.11",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mdi": "^2.2.43",
    "ng-packagr": "^12.2.6",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "release-it": "^12.4.2",
    "rxjs": "~6.5.3",
    "ts-node": "~8.4.1",
    "tslint": "~6.1.0",
    "typescript": "~4.5.4",
    "zone.js": "~0.11.4"
  },
  "ngPackage": {
    "dest": "dist/ngx-stripe",
    "lib": {
      "entryFile": "projects/ngx-stripe/src/public_api.ts"
    }
  }
}